{
  "name": "GTimeTracking",
  "version": "1.1",
  "description": "Track your activities using Google Calendar from Google Chrome",
  "options_page": "options.html",
  "background_page": "background.html",
  "permissions": [
    "notifications", "tabs", "http://*.google.com/", "https://*.google.com/"
   ],
  "browser_action": {
    "default_icon": "images/icon-16.png",
    "popup": "popup.html"
  },
  "icons": {
    "128": "images/icon-128.png",
    "16":"images/icon-16.png"
  }
}